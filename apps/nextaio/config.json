{
  "$schema": "../schema.json",
  "name": "Nextcloud AIO",
  "available": true,
  "exposable": true,
  "port": 8083,
  "id": "nextaio",
  "tipi_version": 62,
  "version": "25.0.12-apache",
  "categories": [
    "data"
  ],
  "description": "Nextcloud is a self-hosted, open source, and fully-featured cloud storage solution for your personal files, office documents, and photos.",
  "short_desc": "Productivity platform that keeps you in control",
  "author": "Nextcloud GmbH",
  "source": "https://github.com/nextcloud/all-in-one",
  "form_fields": [
    {
      "type": "text",
      "label": "Username",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_ADMIN_USER"
    },
    {
      "type": "password",
      "label": "Password",
      "max": 50,
      "min": 10,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_ADMIN_PASSWORD"
    },{
      "type": "text",
      "label": "db name",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_DB_NAME"
    },{
      "type": "text",
      "label": "db host",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_DB_HOST"
    },{
      "type": "text",
      "label": "db user",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_DB_USER"
    },
    {
      "type": "password",
      "label": "DB pass",
      "max": 50,
      "min": 10,
      "required": true,
      "env_variable": "NEXTCLOUD_AIO_DB_PASS"
    }
  ]
}
